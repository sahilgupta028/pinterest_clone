<% include ./partials/header.ejs %>
  <div class="w-full min-h-screen bg-zinc-900 px-10 py-8 text-white">
    <div class="cards flex flex-wrap gap-10">
        <% user.posts.forEach(function(elem){ %>
            <div class=" card w-72">
                <div class="w-full h-52 rounded-lg overflow-hidden">
                    <img src="/images/uploads/<%= elem.image %>" class="w-full h-full object-cover" alt="">
                </div>
                <h4 class="mt-2 text-md font-semibold"><%= elem.title %></h4>
                <h5 class="text-xs mt-2"><%= elem.description %></h5>
            </div>
        <% }) %>
  </div>
  </div>
<% include ./partials/footer.ejs %>